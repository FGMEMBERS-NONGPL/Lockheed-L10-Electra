	<animation>
		<type>pick</type>
		<object-name>FlapsLev</object-name>
		<action>
			<name>Increase</name>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>controls.flapsDown(1)</script>
			</binding>
			<mod-up>
				<binding>
					<command>nasal</command>
					<script>controls.flapsDown(0)</script>
				</binding>
			</mod-up>
		</action>
		<action>
			<name>Decrease</name>
			<button>1</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>controls.flapsDown(-1)</script>
			</binding>
			<mod-up>
				<binding>
					<command>nasal</command>
					<script>controls.flapsDown(0)</script>
				</binding>
			</mod-up>
		</action>
	</animation>

<!-- Rudder Trim -->
	<!-- 	Right -->
	<animation>
		<type>rotate</type>
		<object-name>RTrim</object-name>
		<property>controls/flight/rudder-trim</property>
		<factor>360</factor>
		<center>
			<x-m> 1.737</x-m>
			<y-m>-0.349</y-m>
			<z-m>-0.146</z-m>
		</center>
