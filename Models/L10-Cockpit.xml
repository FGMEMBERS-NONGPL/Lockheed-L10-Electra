			</binding>
		</action>
	</animation>

	<!-- 	Gear Lever-->
	<animation>
		<type>rotate</type>
		<object-name>GearLev</object-name>
		<property>controls/gear/gear-down</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>    0</dep>
