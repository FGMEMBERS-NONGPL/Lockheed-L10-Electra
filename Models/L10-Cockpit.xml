<PropertyList>

	<path>Lockheed-L10-Cockpit.ac</path>

	<model>
		<path>L10-Instruments.xml</path>
	</model>

<!-- Yoke -->
	<animation>
		<type>rotate</type>
		<object-name>Yoke</object-name>
		<object-name>YokeWheelL</object-name>
		<object-name>YokeWheelR</object-name>
		<property>controls/flight/elevator-input</property>
		<factor>-15</factor>
		<center>
			<x-m> 1.860</x-m>
			<y-m>-0.643</y-m>
			<z-m>-0.425</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>


	<animation>
		<type>rotate</type>
		<object-name>YokeWheelL</object-name>
		<property>controls/flight/aileron-input</property>
		<factor>-90</factor>
		<center>
			<x-m> 1.890</x-m>
			<y-m>-0.375</y-m>
			<z-m> 0.175</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
